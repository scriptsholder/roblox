end)
-- crash chat filter
section1:addButton("Crash Chat filter", function()
while true do
    wait(1.9)
local args = {
    [1] = "‚Åü",
    [2] = "All"
}
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
end
end)
